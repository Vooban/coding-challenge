@{
    ViewBag.Title = "Index";
}

<h1>Suggestions</h1>
<div>
    <div ng-controller="suggestionController">
        <table class="table table-bordered table-condensed">
            <tr>
                <td>Enter Search Value:</td>
                <td>
                    <input type="text" ng-model="filterValue" class="form-control" ng-change="getFilteredData()" />
                </td>
            </tr>
        </table>
        <table class="table table-bordered table-condensed table-striped">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Latitude</th>
                    <th>Longitude</th>
                    <th>Score</th>
                </tr>
            </thead>
            <tbody>
                <tr ng-repeat="suggestion in Suggestions">
                    <td>{{suggestion.name}}</td>
                    <td>{{suggestion.lat}}</td>
                    <td>{{suggestion.long}}</td>
                    <td>{{suggestion.score}}</td>
                </tr>
            </tbody>
        </table>
        <span>{{Message}}</span>
    </div>
    
</div>

<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<link href="~/Content/bootstrap-theme.min.css" rel="stylesheet" />
<script src="~/Scripts/jquery-2.2.0.js"></script>
<script src="~/Scripts/tether/tether.js"></script>
<script src="~/Scripts/bootstrap.js"></script>
<script src="~/Scripts/angular.js"></script>
<script src="~/App/app.js"></script>
<script src="~/App/SuggestionApp/suggestionApp.module.js"></script>
<script src="~/App/SuggestionApp/suggestionApp.data.service.js"></script>
<script src="~/App/SuggestionApp/suggestionApp.controller.js"></script>


